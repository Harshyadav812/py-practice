{
  "tasks": [
    {
      "name": "step_1",
      "type": "calculate",
      "operation": "add",
      "numbers": [
        10,
        20
      ]
    },
    {
      "name": "step_2",
      "type": "print",
      "content": "Hello from the engine!"
    },
    {
      "name": "wait_a_bit",
      "type": "delay",
      "seconds": 2
    },
    {
      "name": "step_3",
      "type": "http",
      "method": "GET",
      "url": "https://api.zippopotam.us/us/90210"
    },
    {
      "name": "step_4",
      "type": "calculate",
      "operation": "mul",
      "numbers": [
        "$step_3.places.0.latitude",
        2
      ]
    },
    {
      "name": "step_5",
      "type": "print",
      "content": "The latitude is $step_3.places.0.latitude degrees"
    },
    {
      "name": "check_value",
      "type": "condition",
      "left": "$step_1",
      "operator": ">",
      "right": 25,
      "then_do": {
        "type": "print",
        "content": "Value is greater than 25!"
      },
      "else_do": {
        "type": "print",
        "content": "Value is 25 or less"
      }
    },
    {
      "name": "create_item",
      "type": "http",
      "method": "POST",
      "url": "https://httpbin.org/post",
      "body": {
        "message": "Hello",
        "value": "$step_1"
      }
    },
    {
      "name": "process_places",
      "type": "loop",
      "items": "$step_3.places",
      "as": "current_item",
      "do": {
        "type": "print",
        "content": "City: $current_item.state"
      }
    },
    {
      "name": "fetch_all",
      "type": "parallel",
      "tasks": [
        {
          "type": "http",
          "url": "https://api1.com"
        },
        {
          "type": "http",
          "url": "https://api2.com"
        },
        {
          "type": "http",
          "url": "https://api3.com"
        }
      ]
    }
  ]
}